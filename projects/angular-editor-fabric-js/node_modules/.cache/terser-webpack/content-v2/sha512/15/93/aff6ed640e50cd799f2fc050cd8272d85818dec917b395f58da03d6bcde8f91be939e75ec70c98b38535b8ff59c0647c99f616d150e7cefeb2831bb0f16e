,1);if(0===l)return new Hsva(h,0,0,a);{const v=l+s*(1-Math.abs(2*l-1))/2;return new Hsva(h,2*(v-l)/v,v,a)}}hsvaToRgba(hsva){let r,g,b;const h=hsva.h,s=hsva.s,v=hsva.v,a=hsva.a,i=Math.floor(6*h),f=6*h-i,p=v*(1-s),q=v*(1-f*s),t=v*(1-(1-f)*s);switch(i%6){case 0:r=v,g=t,b=p;break;case 1:r=q,g=v,b=p;break;case 2:r=p,g=v,b=t;break;case 3:r=p,g=q,b=v;break;case 4:r=t,g=p,b=v;break;case 5:r=v,g=p,b=q;break;default:r=0,g=0,b=0}return new Rgba(r,g,b,a)}cmykToRgb(cmyk){return new Rgba((1-cmyk.c)*(1-cmyk.k),(1-cmyk.m)*(1-cmyk.k),(1-cmyk.y)*(1-cmyk.k),cmyk.a)}rgbaToCmyk(rgba){const k=1-Math.max(rgba.r,rgba.g,rgba.b);return 1===k?new Cmyk(0,0,0,1,rgba.a):new Cmyk((1-rgba.r-k)/(1-k),(1-rgba.g-k)/(1-k),(1-rgba.b-k)/(1-k),k,rgba.a)}rgbaToHsva(rgba){let h,s;const r=Math.min(rgba.r,1),g=Math.min(rgba.g,1),b=Math.min(rgba.b,1),a=Math.min(rgba.a,1),max=Math.max(r,g,b),min=Math.min(r,g,b),v=max,d=max-min;if(s=0===max?0:d/max,max===min)h=0;else{switch(max){case r:h=(g-b)/d+(g<b?6:0);break;case g:h=(b-r)/d+2;break;case b:h=(r-g)/d+4;break;default:h=0}h/=6}return new Hsva(h,s,v,a)}rgbaToHex(rgba,allowHex8){let hex=\"#\"+(1<<24|rgba.r<<16|rgba.g<<8|rgba.b).toString(16).substr(1);return allowHex8&&(hex+=(256|Math.round(255*rgba.a)).toString(16).substr(1)),hex}normalizeCMYK(cmyk){return new Cmyk(cmyk.c/100,cmyk.m/100,cmyk.y/100,cmyk.k/100,cmyk.a)}denormalizeCMYK(cmyk){return new Cmyk(Math.floor(100*cmyk.c),Math.floor(100*cmyk.m),Math.floor(100*cmyk.y),Math.floor(100*cmyk.k),cmyk.a)}denormalizeRGBA(rgba){return new Rgba(Math.round(255*rgba.r),Math.round(255*rgba.g),Math.round(255*rgba.b),rgba.a)}stringToHsva(colorString=\"\",allowHex8=!1){let hsva=null;colorString=(colorString||\"\").toLowerCase();const stringParsers=[{re:/(rgb)a?\\(\\s*(\\d{1,3})\\s*,\\s*(\\d{1,3})\\s*%?,\\s*(\\d{1,3})\\s*%?(?:,\\s*(\\d+(?:\\.\\d+)?)\\s*)?\\)/,parse:function(execResult){return new Rgba(parseInt(execResult[2],10)/255,parseInt(execResult[3],10)/255,parseInt(execResult[4],10)/255,isNaN(parseFloat(execResult[5]))?1:parseFloat(execResult[5]))}},{re:/(hsl)a?\\(\\s*(\\d{1,3})\\s*,\\s*(\\d{1,3})%\\s*,\\s*(\\d{1,3})%\\s*(?:,\\s*(\\d+(?:\\.\\d+)?)\\s*)?\\)/,parse:function(execResult){return new Hsla(parseInt(execResult[2],10)/360,parseInt(execResult[3],10)/100,parseInt(execResult[4],10)/100,isNaN(parseFloat(execResult[5]))?1:parseFloat(execResult[5]))}}];stringParsers.push(allowHex8?{re:/#([a-fA-F0-9]{2})([a-fA-F0-9]{2})([a-fA-F0-9]{2})([a-fA-F0-9]{2})?$/,parse:function(execResult){return new Rgba(parseInt(execResult[1],16)/255,parseInt(execResult[2],16)/255,parseInt(execResult[3],16)/255,parseInt(execResult[4]||\"FF\",16)/255)}}:{re:/#([a-fA-F0-9]{2})([a-fA-F0-9]{2})([a-fA-F0-9]{2})$/,parse:function(execResult){return new Rgba(parseInt(execResult[1],16)/255,parseInt(execResult[2],16)/255,parseInt(execResult[3],16)/255,1)}}),stringParsers.push({re:/#([a-fA-F0-9])([a-fA-F0-9])([a-fA-F0-9])$/,parse:function(execResult){return new Rgba(parseInt(execResult[1]+execResult[1],16)/255,parseInt(execResult[2]+execResult[2],16)/255,parseInt(execResult[3]+execResult[3],16)/255,1)}});for(const key in stringParsers)if(stringParsers.hasOwnProperty(key)){const parser=stringParsers[key],match=parser.re.exec(colorString),color=match&&parser.parse(match);if(color)return color instanceof Rgba?hsva=this.rgbaToHsva(color):color instanceof Hsla&&(hsva=this.hsla2hsva(color)),hsva}return hsva}outputFormat(hsva,outputFormat,alphaChannel){switch(\"auto\"===outputFormat&&(outputFormat=hsva.a<1?\"rgba\":\"hex\"),outputFormat){case\"hsla\":const hsla=this.hsva2hsla(hsva),hslaText=new Hsla(Math.round(360*hsla.h),Math.round(100*hsla.s),Math.round(100*hsla.l),Math.round(100*hsla.a)/100);return hsva.a<1||\"always\"===alphaChannel?\"hsla(\"+hslaText.h+\",\"+hslaText.s+\"%,\"+hslaText.l+\"%,\"+hslaText.a+\")\":\"hsl(\"+hslaText.h+\",\"+hslaText.s+\"%,\"+hslaText.l+\"%)\";case\"rgba\":const rgba=this.denormalizeRGBA(this.hsvaToRgba(hsva));return hsva.a<1||\"always\"===alphaChannel?\"rgba(\"+rgba.r+\",\"+rgba.g+\",\"+